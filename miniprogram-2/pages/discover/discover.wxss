.container {
  display: flex;
  flex-direction: column;
  height: 100vh;
  padding: 10px;
}

.menu {
  flex: 1;
}

.menu-group {
  margin-bottom: 20px;
}

.menu-title {
  font-size: 18px;
  font-weight: bold;
  margin-bottom: 10px;
}

.menu-item {
  background-color: #f0f0f0;
  border-radius: 5px;
  padding: 5px 10px;
  margin-bottom: 5px;
}
